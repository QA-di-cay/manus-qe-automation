    "eslint-plugin-prettier": "^5.5.4",
    "otplib": "^12.0.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
