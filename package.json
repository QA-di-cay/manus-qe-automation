    "test:ticketSellMode:bs": "browserstack-node-sdk playwright test -g '@ticketSellMode'",
    "test:geofenceRange:bs": "browserstack-node-sdk playwright test -g '@geofenceRange'",
    "test:settings:bs": "browserstack-node-sdk playwright test -g '@settings'",
    "test:dataIntegrations:bs": "browserstack-node-sdk playwright test -g '@dataIntegrations'",
    "test:icons:bs": "browserstack-node-sdk playwright test -g '@icons'",
    "test:regression:bs": "browserstack-node-sdk playwright test -g '@regression'",
